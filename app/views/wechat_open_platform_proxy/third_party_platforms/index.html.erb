<p id="notice"><%= notice %></p>

<h1>Third Party Platforms</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @third_party_platforms.each do |third_party_platform| %>
      <tr>
        <td><%= link_to 'Show', third_party_platform %></td>
        <td><%= link_to 'Edit', edit_third_party_platform_path(third_party_platform) %></td>
        <td><%= link_to 'Destroy', third_party_platform, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Third Party Platform', new_third_party_platform_path %>
